@Article{Ellner1986,
  author    = {Ellner, Stephen},
  title     = {Germination dimorphisms and parent-offspring conflict in seed germination},
  journal   = {J. Theor. Biol.},
  year      = {1986},
  volume    = {123},
  number    = {2},
  pages     = {173--185},
  month     = nov,
  issn      = {0022-5193},
  abstract  = {Certain plant species are suggested to have a conflict between parent and offspring over the timing of delayed germination, which the parent "wins" by using the seed coat or other structure to keep the embryo from germinating. The conflict occurs because selection on parental genotypes favors delayed germination to reduce competition between offspring, whereas embryos whose germination is delayed have a lower inclusive fitness than those with undelayed germination. Thus, selection on embryo genotypes favors undelayed germination. This contrasts with Westoby's (1981) theory, in which the conflict occurs because of environmental variability, and with Ellner & Shmida's (1981) theory of dormancy as sib-sib altruism. It is argued that those theories are untenable, and that the present hypothesis provides the best available explanation for some aspects of germination dimorphisms and the control of delayed germination.},
  doi       = {10.1016/S0022-5193(86)80151-5},
  owner     = {bram},
  timestamp = {2011-03-07},
  url       = {http://dx.doi.org/10.1016/S0022-5193(86)80151-5},
}

@Article{Ellner1994,
  author    = {Ellner, Stephen and Hairston, Nelson G. Jr.},
  title     = {Role of overlapping generations in maintaining genetic variation in a fluctuating environment},
  journal   = {Am. Nat.},
  year      = {1994},
  volume    = {143},
  number    = {3},
  pages     = {403--417},
  month     = mar,
  issn      = {00030147},
  abstract  = {Population genetics theory suggests that temporally fluctuating selection on phenotypes can act to maintain genetic variance only under very restrictive conditions. However, this conclusion is based on models with discrete nonoverlapping generations. We propose here that temporally fluctuating selection can indeed contribute significantly to the maintenance of genetic variation when the effects of overlapping generations and age-specific or stage-specific selection are considered. We develop a simple model for a population with overlapping generations, experiencing stabilizing selection with a temporally fluctuating optimum, and subject to repeated invasions by mutants with alternative phenotypes. We find that an evolutionarily stable population must have positive genetic variance maintained by selection so long as the product (variance of fluctuations) times (amount of generation overlap) times (selection intensity) is sufficiently high. This result applies to haploid, diploid, single-locus, or multilocus inheritance, and it does not depend on any form of heterozygote advantage to maintain genetic variance. However, it depends on the map between genotype and phenotype being constrained. If a single genotype can produce an arbitrary distribution of phenotypes, then genetic variance is not maintained by selection.},
  owner     = {bram},
  publisher = {The University of Chicago Press for The American Society of Naturalists},
  timestamp = {2011-03-18},
  url       = {http://www.jstor.org/stable/2462737},
}

