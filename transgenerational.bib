@Article{Benus1987,
  Title                    = {Individual Differences in Behavioural Reaction to a Changing Environment in Mice and Rats},
  Author                   = {Benus, R. F. and Koolhaas, J. M. and Oortmerssen, G. A. van},
  Journal                  = {Behaviour},
