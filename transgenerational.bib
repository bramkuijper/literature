@Article{Guillaume2009,
  author    = {Guillaume, Fr{\'e}d{\'e}ric and Perrin, Nicolas},
  title     = {Inbreeding load, bet hedging, and the evolution of sex-biased dispersal},
  journal   = {Am. Nat.},
  year      = {2009},
  volume    = {173},
