@Article{Gulisija2016,
  author    = {Gulisija, Davorka and Kim, Yuseob and Plotkin, Joshua B.},
  title     = {Phenotypic plasticity promotes balanced polymorphism in periodic environments by a genomic storage effect},
  journal   = {Genetics},
  year      = {2016},
  volume    = {202},
  number    = {4},
  pages     = {1437--1448},
  issn      = {0016-6731},
  abstract  = {Phenotypic plasticity is known to evolve in perturbed habitats, where it alleviates the deleterious effects of selection. But the effects of plasticity on levels of genetic polymorphism, an important precursor to adaptation in temporally varying environments, are unclear. Here we develop a haploid, two-locus population-genetic model to describe the interplay between a plasticity modifier locus and a target locus subject to periodically varying selection. We find that the interplay between these two loci can produce a {\textquotedblleft}genomic storage effect{\textquotedblright} that promotes balanced polymorphism over a large range of parameters, in the absence of all other conditions known to maintain genetic variation. The genomic storage effect arises as recombination allows alleles at the two loci to escape more harmful genetic backgrounds and associate in haplotypes that persist until environmental conditions change. Using both Monte Carlo simulations and analytical approximations we quantify the strength of the genomic storage effect across a range of selection pressures, recombination rates, plasticity modifier effect sizes, and environmental periods.},
  doi       = {10.1534/genetics.115.185702},
  eprint    = {http://www.genetics.org/content/202/4/1437.full.pdf},
  owner     = {bram},
  publisher = {Genetics},
