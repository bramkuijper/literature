@Article{Mott2014,
  Title                    = {The architecture of parent-of-origin effects in mice},
  Author                   = {Mott, Richard and Yuan, Wei and Kaisaki, Pamela and Gan, Xiangchao and Cleak, James and Edwards, Andrew and Baud, Amelie and Flint, Jonathan},
  Journal                  = {Cell},
  Year                     = {2014},

  Month                    = jan,
  Number                   = {1, 2},
  Pages                    = {332--342},
  Volume                   = {156},

  Abstract                 = {Summary The number of imprinted genes in the mammalian genome is predicted to be small, yet we show here, in a survey of 97 traits measured in outbred mice, that most phenotypes display parent-of-origin effects that are partially confounded with family structure. To address this contradiction, using reciprocal F1 crosses, we investigated the effects of knocking out two nonimprinted candidate genes, Man1a2 and H2-ab1, that reside at nonimprinted loci but that show parent-of-origin effects. We show that expression of multiple genes becomes dysregulated in a sex-, tissue-, and parent-of-origin-dependent manner. We provide evidence that nonimprinted genes can generate parent-of-origin effects by interaction with imprinted loci and deduce that the importance of the number of imprinted genes is secondary to their interactions. We propose that this gene network effect may account for some of the missing heritability seen when comparing sibling-based to population-based studies of the phenotypic effects of genetic variants.},
  Doi                      = {10.1016/j.cell.2013.11.043},
  ISSN                     = {0092-8674},
  Owner                    = {bram},
  timestamp = {2015-03-17},
  Url                      = {http://dx.doi.org/10.1016/j.cell.2013.11.043}
}

