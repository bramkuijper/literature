@Article{Gasch2000,
  author    = {Gasch, Audrey P. and Spellman, Paul T. and Kao, Camilla M. and Carmel-Harel, Orna and Eisen, Michael B. and Storz, Gisela and Botstein, David and Brown, Patrick O.},
