@Article{Thiede1998,
  Title                    = {Maternal inheritance and its effect on adaptive evolution: a quantitative genetic analysis of maternal effects in a natural plant population},
  Author                   = {Thiede, Denise A.},
  Journal                  = {Evolution},
  Year                     = {1998},

  Month                    = aug,
  Number                   = {4},
  Pages                    = {998--1015},
  Volume                   = {52},

  Abstract                 = {A mother can influence a trait in her offspring both by the genes she transmits (Mendelian inheritance) and by maternal attributes that directly affect that trait in her offspring (maternal inheritance). Maternal inheritance can alter the direction, rate, and duration of adaptive evolution from standard Mendelian models and its impact on adaptive evolution is virtually unexplored in natural populations. In a hierarchical quantitative genetic analysis to determine the magnitude and structure of maternal inheritance in the winter annual plant, Collinsia verna, I consider three potential models of inheritance. These range from a standard Mendelian model estimating only direct (i.e., Mendelian) additive and environmental variance components to a maternal inheritance model estimating six additive and environmental variance components: direct additive (Ï2 Ao ) and environmental (Ï2 Eo ) variances; maternal additive (Ï2 Am ) and environmental (Ï2 Em ) variances; and the direct-maternal additive (ÏAoA m ) and environmental (ÏEoE m ) covariances. The structure of maternal inheritance differs among the 10 traits considered at four stages in the life cycle. Early in the life cycle, seed weight and embryo weight display substantial Ï2 Am , a negative ÏAoA m , and a positive ÏEoE m . Subsequently, cotyledon diameter displays Ï2 Ao and Ï2 Am of roughly the same magnitude and negative ÏAoA m . For fall rosettes, leaf number and length are best described by a Mendelian model. In the spring, leaf length displays maternal inheritance with significant Ï2 Ao and Ï2 Am and a negative ÏAoA m . All maternally inherited traits show significant negative ÏAoA m . Predicted response to selection under maternal inheritance depends on Ï2 Ao and Ï2 Am as well as ÏAoA m . Negative ÏAoA m results in predicted responses in the opposite direction to selection for seed weight and embryo weight and predicted responses near zero for all subsequent maternally inherited traits. Maternal inheritance persists through the life cycle of this annual plant for a number of size-related traits and will alter the direction and rate of evolutionary response in this population.},
  Doi                      = {10.2307/2411232},
  ISSN                     = {00143820},
  Owner                    = {bram},
  Publisher                = {Society for the Study of Evolution},
  timestamp = {2011-07-25},
  Url                      = {http://dx.doi.org/10.2307/2411232}
}

