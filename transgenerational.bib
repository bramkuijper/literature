@Article{Eising2006,
  author    = {Eising, Corine M and M{\"u}ller, Wendt and Groothuis, Ton G.G},
  title     = {Avian mothers create different phenotypes by hormone deposition in their eggs},
  journal   = {Biol. Lett.},
