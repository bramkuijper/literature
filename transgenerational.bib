@Article{Cozzolino2005,
  author    = {Cozzolino, Salvatore and Widmer, Alex},
  title     = {Orchid diversity: an evolutionary consequence of deception?},
  journal   = {Trends Ecol. Evol.},
  year      = {2005},
  volume    = {20},
