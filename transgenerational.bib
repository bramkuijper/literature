@Article{Cohen1976,
  author    = {Cohen, Joel E.},
  title     = {Ergodicity of Age Structure in Populations with Markovian Vital Rates, I: Countable States},
  journal   = {J. Amer. Stat. Assoc.},
  year      = {1976},
  volume    = {71},
  number    = {354},
  pages     = {335--339},
  month     = jun,
  issn      = {01621459},
  abstract  = {This paper establishes new ergodic theorems for population age structure. Let A<sub>1</sub>, A<sub>2</sub>, ... be denumerable Leslie matrices, and for  = 1, 2, m<sup>()</sup>(0) be age structures (vectors with elements m<sub>i</sub><sup>()</sup>(0)), satisfying the assumptions of the Coale-Lopez theorem. Let {A(t, Ï)}<sup></sup><sub>t=1</sub> be sample paths of a discrete-time Markov chain with sample space {A<sub>k</sub>}<sup></sup><sub>k=1</sub>, and m<sup>()</sup>(t) = A(t, Ï<sub></sub>) ¯ A(t - 1, Ï<sub>l</sub>) ¯ A(1, Ï<sub>l</sub>)m<sup>l</sup>)(0). Then for b = 1, 2, ... (weak stochastic ergodicity) <tex-math>$\lim_{t \rightarrow\infty}(E(m_j^{(1)}(t)/m_i^{(1)}(t))^b - E(m_j^{(2)}(t)/m_i^{(2)}(t))^b) = 0$</tex-math> if the chain is finite and weakly ergodic (see [4]) or denumerable and weakly ergodic (see [13]). The limit holds and (strong stochastic ergodicity) /{{m}<sup>()</sup>(t)}<sup></sup><sub>t = 1</sub> converge in distribution, if the chain is homogeneous, aperiodic, positive recurrent, and uniformly geometrically ergodic.},
  owner     = {bram},
  publisher = {Taylor \& Francis, Ltd. on behalf of the American Statistical Association},
  timestamp = {2015-04-23},
  url       = {http://www.jstor.org/stable/2285308},
}

@Article{Cohen1977,
  author    = {Cohen, Joel E.},
  journal   = {Adv. Appl. Prob.},
  title     = {Ergodicity of age structure in populations with {Markovian} vital rates. {II}. {General} states},
  year      = {1977},
  issn      = {00018678},
  month     = mar,
  number    = {1},
  pages     = {18--37},
  volume    = {9},
  abstract  = {The age structure of a large, unisexual, closed population is described here by a vector of the proportions in each age class. Non-negative matrices of age-specific birth and death rates, called Leslie matrices, map the age structure at one point in discrete time into the age structure at the next. If the sequence of Leslie matrices applied to a population is a sample path of an ergodic Markov chain, then: (i) the joint process consisting of the age structure vector and the Leslie matrix which produced that age structure is a Markov chain with explicit transition function; (ii) the joint distribution of age structure and Leslie matrix becomes independent of initial age structure and of the initial distribution of the Leslie matrix after a long time; (iii) when the Markov chain governing the Leslie matrix is homogeneous, the joint distribution in (ii) approaches a limit which may be easily calculated as the solution of a renewal equation. A numerical example will be given in Cohen (1977).},
  owner     = {bram},
  publisher = {Applied Probability Trust},
  timestamp = {2015-04-23},
  url       = {http://www.jstor.org/stable/1425814},
}

@Article{Cohen1977a,
