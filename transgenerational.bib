@Article{Pilz2003,
  author    = {Kevin M. Pilz and Henrik G. Smith and Maria I. Sandell and Hubert Schwabl},
  journal   = {Anim. Behav.},
  title     = {Interfemale variation in egg yolk androgen allocation in the {\uppercase{E}uropean} starling: do high-quality females invest more?},
  year      = {2003},
  issn      = {0003-3472},
  number    = {4},
  pages     = {841 - 850},
  volume    = {65},
  abstract  = {Androgenic hormones occur naturally in bird egg yolk and are known to enhance growth in canary and gull chicks. Gil et al. (1999, Science, 286, 126â128) have recently proposed that female allocation of androgens to eggs represents a form of costly maternal investment. This hypothesis predicts that females of high quality or high reproductive potential should invest more yolk androgens in their eggs than females of low quality or low reproductive potential. We tested these predictions by examining interfemale variation in allocation of the androgenic steroids testosterone and androstenedione to eggs in a wild population of European starlings, Sturnus vulgaris. We collected 30 full clutches of eggs and captured 22 of the females that laid these clutches for phenotypic measurements. In agreement with the hypothesis, we found that there was significant interfemale variation in yolk androgen concentrations. Furthermore, older females deposited more androstenedione and testosterone in eggs than 1-year-old females, and females laying early or large clutches deposited more testosterone in eggs than females laying late or small clutches. However, females in good body condition did not deposit more androgens in eggs than females in poor body condition. Large-yolked eggs had a higher total yolk androgen content than small-yolked eggs, but larger eggs did not have higher concentrations of yolk androgens. The results in general support the investment hypothesis of yolk androgen allocation. However, further data are needed regarding the costs of yolk androgen allocation and the effects of yolk androgens on offspring to substantiate the assumptions of the hypothesis. Copyright 2003 Published by Elsevier Science Ltd on behalf of The Association for the Study of Animal Behaviour.},
  doi       = {10.1006/anbe.2003.2094},
  owner     = {bram},
  timestamp = {2016-07-08},
}

