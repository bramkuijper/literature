@Article{Fritzsche2014,
  author    = {Fritzsche, K. and Timmermeyer, N. and Wolter, M. and Michiels, N. K.},
  title     = {Female, but not male, nematodes evolve under experimental sexual coevolution},
  journal   = {Proc. R. Soc. Lond. B Biol. Sci.},
  year      = {2014},
  volume    = {281},
  number    = {1796},
  issn      = {0962-8452},
  abstract  = {Coevolution between the sexes is often considered to be male-driven: the male genome is constantly scanned by selection for traits that increase relative male fertilization success. Whenever these traits are harmful to females, the female genome is scanned for resistance traits. The resulting antagonistic coevolution between the sexes is analogous to Red Queen dynamics, where adaptation and counteradaptation keep each other in check. However, the underlying assumption that male trait evolution precedes female trait counteradaptation has received few empirical tests. Using the gonochoristic nematode Caenorhabditis remanei, we now show that 20 generations of relaxed versus increased sexual selection pressure lead to female, but not to male, trait evolution, questioning the generality of a male-driven process.},
