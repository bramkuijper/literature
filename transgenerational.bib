@Article{Rankin2015,
  author    = {Rankin, Catharine H.},
  title     = {A review of transgenerational epigenetics for {\uppercase{RNA}i}, longevity, germline maintenance and olfactory imprinting in {\emph{\uppercase{C}aenorhabditis elegans}}},
