@Article{Cheverud1983,
  Title                    = {Quantitative genetics of development: genetic correlations among age-specific trait values and the evolution of ontogeny},
  Author                   = {Cheverud, James M. and Rutledge, J. J. and Atchley, William R.},
  Journal                  = {Evolution},
  Year                     = {1983},

  Month                    = sep,
  Number                   = {5},
  Pages                    = {895--905},
  Volume                   = {37},

  Abstract                 = {Genetic correlations among age-specific weights and tail lengths in a laboratory population of rats are presented in order to investigate variable gene action in ontogeny and the effects of variable gene action on ontogenetic response to selection. Phenotypic, maternal, and environmental correlations were also calculated and compared to the genetic correlations. Three basic kinds of gene effects on ontogeny are identified: (1) gene effects which cause an individual to be either larger or smaller than average throughout ontogeny; (2) gene effects which cause an individual to be smaller than average early in ontogeny and larger than average late in ontogeny, and vice versa; and (3) gene effects acting only at one or two consecutive ages. Gene effects which are constant throughout ontogeny account for most of the genetic variance in ontogeny and cause variation in average growth curve height among individuals. Gene effects which are different early and late in ontogeny account for a smaller, but still significant portion of the genetic variance in ontogeny and cause variation in growth curve shape, independent of variation in growth curve height. Genes without pleiotropic effects in ontogeny account for only a minor portion of the genetic variance in ontogeny. The effects of variable gene action on ontogenetic response to selection are demonstrated by a simulation of the correlated at various ages. Phenotypic, maternal, and environmental correlation patterns were all similar to the genetic correlation pattern. The indices of integration are higher for the maternal and genetic matrices than for the phenotypic and, especially, the environmental matrices. Therefore, ontogenetic stages are more closely linked in evolution than one would infer based on the phenotypic correlations alone. The patterns of genetic, phenotypic, maternal, and environmental correlation are similar for age-specific weights and tail lengths.},
  Doi                      = {10.2307/2408405},
  ISSN                     = {00143820},
  Owner                    = {thuis},
  Publisher                = {Society for the Study of Evolution},
  timestamp = {2013-07-16},
  Url                      = {http://dx.doi.org/10.2307/2408405}
}

@Article{Chevin2010b,
  author    = {Chevin, Luis-Miguel},
  title     = {On measuring selection in experimental evolution},
  journal   = {Biol. Lett.},
  year      = {2010},
  volume    = {7},
  pages     = {210--213},
  issn      = {1744-9561},
  abstract  = {Distributions of mutation fitness effects from evolution experiments are available in an increasing number of species, opening the way for a vast array of applications in evolutionary biology. However, comparison of estimated distributions among studies is hampered by inconsistencies in the definitions of fitness effects and selection coefficients. In particular, the use of ratios of Malthusian growth rates as {\textquoteleft}relative fitnesses{\textquoteright} leads to wrong inference of the strength of selection. Scaling Malthusian fitness by the generation time may help overcome this shortcoming, and allow accurate comparison of selection coefficients across species. For species reproducing by binary fission (neglecting cellular death), ln2 can be used as a correction factor, but in general, the growth rate and generation time of the wild-type should be provided in studies reporting distribution of mutation fitness effects. I also discuss how density and frequency dependence of population growth affect selection and its measurement in evolution experiments.},
  doi       = {10.1098/rsbl.2010.0580},
  owner     = {bram},
  publisher = {The Royal Society},
  timestamp = {2015-09-22},
}

