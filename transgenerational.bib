@Article{Teixeira2009,
  Title                    = {A role for {\uppercase{RNA}i} in the selective correction of {\uppercase{DNA}} methylation defects},
  Author                   = {Teixeira, Felipe Karam and Heredia, Fabiana and Sarazin, Alexis and Roudier, Fran{\,c}ois and Boccara, Martine and Ciaudo, Constance and Cruaud, Corinne and Poulain, Julie and Berdasco, Maria and Fraga, Mario F. and Voinnet, Olivier and Wincker, Patrick and Esteller, Manel and Colot, Vincent},
  Journal                  = {Science},
