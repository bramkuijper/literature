@Article{Gillespie1981,
  author    = {Gillespie, John H.},
  title     = {The Role of Migration in the Genetic Structure of Populations in Temporally and Spatially Varying Environments. III. Migration Modification},
  journal   = {Am. Nat.},
  year      = {1981},
  volume    = {117},
  number    = {3},
  pages     = {223--233},
  month     = mar,
  issn      = {00030147},
  abstract  = {The foregoing analysis makes use of various bits of analyses and simulations which, taken together, suggest that evolution may act on migration rates in such a way as to maximize the geometric mean fitness of the population. This maximization involves a decrease in migration when there is, in some sense, more spatial than temporal variation in the environment and an increase when there is more temporal than spatial variation. There may or may not be an intermediate level of migration which maximizes the geometric mean fitness depending on the values of the various parameters. As with other modifier loci the rate of change in frequency of the migration alleles is very small, of the order of the product of the linkage disequilibrium and the selection coefficient of the primary locus. The analysis is only concerned with a single selected locus. If there were many loci experiencing fluctuating selection then an intermediate level of migration may generally be optimal if there is locus-by-locus variation in the relative amounts of temporal versus spatial fluctuations. As a final point, the general impression from much of the work on modifier theory is that selection will usually either maximize or minimize the parameters under consideration (Karlin and McGregor 1974). It may be, however, that in fluctuating environments intermediate levels of the parameters are achieved by modification. There is a suggestion that this is the case for the modification of recombination in the simulations of Charlesworth (1976). If this proves to be the case in general, then a good deal of the explanation for the values of genetic parameters observed in natural populations may reside with the effects of fluctuating environments.},
  owner     = {bram},
  publisher = {The University of Chicago Press for The American Society of Naturalists},
  timestamp = {2011-03-13},
  url       = {http://www.jstor.org/stable/2460524},
}

@Article{Gillespie1981a,
  Title                    = {Mutation modification in a random environment},
  Author                   = {Gillespie, John H.},
  Journal                  = {Evolution},
  Year                     = {1981},

  Month                    = may,
  Number                   = {3},
  Pages                    = {468--476},
  Volume                   = {35},

  Abstract                 = {The action of natural selection on mutation rates in a population in a temporally fluctuating environment is examined. The model incorporates a modifier locus that affects the mutation rate at a primary locus which is subject to fluctuating selection pressures. Both Monte-Carlo simulations and a loose-linkage analysis of the appropriate diffusion approximation of the model indicate that selection on mutation rates will operate toward three goals: the highest possible mutation rate, the lowest possible mutation rate, or an intermediate mutation rate. The actual goal is determined by the two parameters used to describe the action of selection on the primary locus. All of these adjustments of the mutation rates are done through individual rather than group selection.},
  Doi                      = {10.2307/2408195},
  ISSN                     = {00143820},
  Owner                    = {bram},
  Publisher                = {Society for the Study of Evolution},
  timestamp = {2011-07-18},
  Url                      = {http://dx.doi.org/10.2307/2408195}
}

@Article{Gillespie1978a,
  author    = {Gillespie, John H. and Guess, Harry A.},
  title     = {The effects of environmental autocorrelations on the progress of selection in a random environment},
  journal   = {Am. Nat.},
  year      = {1978},
  volume    = {112},
  number    = {987},
  pages     = {897--909},
  month     = sep,
  issn      = {00030147},
  abstract  = {A heuristic method for obtaining Markovian approximations to the solutions of non-Markovian stochastic difference equations is described. The technique is based on recent results given in Guess and Gillespie (1977). The approximating Markov processes are either diffusions or else the solutions to ordinary differential equations with parameters representable by diffusions. The appropriate approximating process is determined by the strength of the autocorrelation. By applying this technique to the problem of selection in random environments, certain useful observations are made. In general, heterozygosity decreases with increasing autocorrelation in the environment. For moderately and strongly autocorrelated environments, the effects of increasing the autocorrelation are analogous to the effects of increasing the variance. This is not true, however, for weakly autocorrelated environments. Finally, the analysis of discrete-time models by continuous-time approximations based on the Stratonovich calculus is shown to give misleading results.},
  owner     = {bram},
  publisher = {The University of Chicago Press for The American Society of Naturalists},
  timestamp = {2013-10-29},
  url       = {http://www.jstor.org/stable/2460165},
}

@Article{Ginsburg2009,
  author    = {Ginsburg, Simona and Jablonka, Eva},
  title     = {Epigenetic learning in non-neural organisms},
  journal   = {J. Biosci.},
  year      = {2009},
