@Article{McNamara2003,
  author    = {McNamara, John M. and Houston, Alasdair I. and Barta, Zolt{\'a}n and Osorno, Jos{\'e}-Luis},
  title     = {Should young ever be better off with one parent than with two?},
  journal   = {Behav. Ecol.},
  year      = {2003},
  volume    = {14},
