@Article{Ruel1999,
  author    = {Ruel, Jonathan J. and Ayres, Matthew P.},
  title     = {Jensen's inequality predicts effects of environmental variation},
  journal   = {Trends Ecol. Evol.},
  year      = {1999},
  volume    = {14},
  number    = {9},
  pages     = {361--366},
  month     = sep,
  issn      = {0169-5347},
  abstract  = {Many biologists now recognize that environmental variance can exert important effects on patterns and processes in nature that are independent of average conditions. Jensenâs inequality is a mathematical proof that is seldom mentioned in the ecological literature but which provides a powerful tool for predicting some direct effects of environmental variance in biological systems. Qualitative predictions can be derived from the form of the relevant response functions (accelerating versus decelerating). Knowledge of the frequency distribution (especially the variance) of the driving variables allows quantitative estimates of the effects. Jensenâs inequality has relevance in every field of biology that includes nonlinear processes.},
  doi       = {10.1016/s0169-5347(99)01664-x},
  keywords  = {Jensen's inequality, Environmental variance, Aggregation bias, Kaufmann effect, Host variance, Plantâherbivore interactions, Photosynthesis, Metabolic rate},
