@Article{McNamara2010a,
  author    = {McNamara, John M. and Leimar, Olof},
  journal   = {Philos. Trans. R. Soc. Lond. B Biol. Sci.},
  title     = {Variation and the response to variation as a basis for successful cooperation},
  year      = {2010},
  issn      = {0962-8436},
  number    = {1553},
  pages     = {2627--2633},
  volume    = {365},
  abstract  = {In applying game theory to problems in biology, differences between individuals are often ignored. In particular, when analysing the evolution of cooperation it is often implicitly assumed that ignoring variation will produce predictions that approximate the solution when differences are included. This need not be true. As we demonstrate, differences are not innocuous noise, but can fundamentally change the nature of a game. Even small amounts of variability can stabilize cooperation by, for example, maintaining the need to deal with cheaters. Differences promote the need to learn about others in an interaction, leading to contingent behaviour that can reduce conflict, and to negotiated outcomes that may or may not be more cooperative than unconditional actions. Once there are mechanisms such as mutation and environmental influences that maintain variation within populations, whether cooperation evolves may depend on the variation in the cooperativeness trait. Variation means that it may be worth taking a chance that a partner is cooperative by being cooperative. When there are markets, so that individuals can break off interactions to seek a better partner, variation promotes choosiness and hence penalizes those uncooperative individuals, who are rejected. Variation promotes the need to monitor the previous behaviour of others, and once this social sensitivity exists, the need to maintain a good reputation can promote cooperation.},
  doi       = {10.1098/rstb.2010.0159},
  eprint    = {http://rstb.royalsocietypublishing.org/content/365/1553/2627.full.pdf},
  publisher = {The Royal Society},
  timestamp = {2023-11-01},
  url       = {http://rstb.royalsocietypublishing.org/content/365/1553/2627},
}

