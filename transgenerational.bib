@Article{Pakyari2015,
  author    = {Pakyari, Hajar and Enkegaard, Annie},
  title     = {Sublethal and transgenerational effects of abamectin on the biological performance of the predatory thrips {\emph{\uppercase{S}colothrips longicornis}} ({\uppercase{T}hysanoptera: \uppercase{T}hripidae})},
