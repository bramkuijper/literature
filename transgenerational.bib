@Article{Boots2011,
  author    = {Boots, Mike},
  title     = {The evolution of resistance to a parasite is determined by resources},
  journal   = {Am. Nat.},
  year      = {2011},
  volume    = {178},
  number    = {2},
  pages     = {214--220},
  month     = aug,
  issn      = {00030147},
  abstract  = {Abstract Given the ubiquity of parasites, it is critical to understand the evolution of defense against them. Using a selection experiment performed across a broad range of host resources, I examine how resistance and associated costs depend on resource availability. Higher resistance to a natural viral pathogen evolves in a host when there are more resources, and this directly suggests a resource-dependent cost of the evolution of resistance. Resistance is traded off with host growth rate, and the costs are stronger under poor resource environments, although adaptation to poor environments reduces these costs. The level of resistance and the costs that are paid for this resistance depend on both the selection environment and the environment in which hosts are assayed, implying that different resistance mechanisms may evolve in different environments. More broadly, the results emphasize that environmental heterogeneity in time and space may underpin variation in immune diversity.},
  owner     = {bram},
  publisher = {The University of Chicago Press for The American Society of Naturalists},
  timestamp = {2012-08-08},
