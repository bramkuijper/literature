@Article{Heath2003,
  Title                    = {Rapid evolution of egg size in captive salmon},
  Author                   = {Heath, Daniel D. and Heath, John W. and Bryden, Colleen A. and Johnson, Rachel M. and Fox, Charles W.},
  Journal                  = {Science},
