@Article{Martins2015,
  author    = {Martins, Bruno M. C. and Locke, James C. W.},
  title     = {Microbial individuality: how single-cell heterogeneity enables population level strategies},
  journal   = {Curr. Opin. Microbiol.},
  year      = {2015},
  volume    = {24},
  number    = {0},
  pages     = {104--112},
  month     = apr,
  issn      = {1369-5274},
  abstract  = {Much of our knowledge of microbial life is only a description of average population behaviours, but modern technologies provide a more inclusive view and reveal that microbes also have individuality. It is now acknowledged that isogenic cell-to-cell heterogeneity is common across organisms and across different biological processes. This heterogeneity can be regulated and functional, rather than just reflecting tolerance to noisy biochemistry. Here, we review recent advances in our understanding of microbial heterogeneity, with an emphasis on the pervasiveness of heterogeneity, the mechanisms that sustain it, and how heterogeneity enables collective function.},
