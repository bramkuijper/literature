@Article{Aiba1998,
  author    = {Aiba, Hirofumi and Kawaura, Ryosuke and Yamamoto, Eiji and Yamada, Hisami and Takegawa, Kaoru and Mizuno, Takeshi},
  title     = {Isolation and characterization of high-osmolarity-sensitive mutants of fission yeast},
  journal   = {J. Bacteriol.},
