@Article{Hastings1979,
  author =    {Hastings, Alan and Caswell, Hal},
  title =     {Role of environmental variability in the evolution of life history strategies},
  journal =   {Proc. Natl. Acad. Sci. USA.},
  year =      {1979},
  volume =    {76},
  number =    {9},
  pages =     {4700--4703},
