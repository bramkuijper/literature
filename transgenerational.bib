@Article{Turan2010,
  author    = {Turan, Nahid and Katari, Sunita and Gerson, Leigh F. and Chalian, Raffi and Foster, Michael W. and Gaughan, John P. and Coutifaris, Christos and Sapienza, Carmen},
  journal   = {PLoS Genet.},
  title     = {Inter- and intra-individual variation in allele-specific {\uppercase{DNA}} methylation and gene expression in children conceived using assisted reproductive technology},
