@Article{Greer2015,
  author =    {Greer, Eric Lieberman and Blanco, Mario Andres and Gu, Lei and Sendinc, Erdem and Liu, Jianzhao and Aristiz{\'a}bal-Corrales, David and Hsu, Chih-Hung and Aravind, L. and He, Chuan and Shi, Yang},
  title =     {DNA Methylation on N6-Adenine in C. elegans},
  journal =   {Cell},
  year =      {2015},
  volume =    {161},
  number =    {4},
  pages =     {868--878},
  month =     may,
  abstract =  {Summary In mammalian cells, DNA methylation on the fifth position of cytosine (5mC) plays an important role as an epigenetic mark. However, DNA methylation was considered to be absent in C.Â elegans because of the lack of detectable 5mC, as well as homologs of the cytosine DNA methyltransferases. Here, using multiple approaches, we demonstrate the presence of adenine N6-methylation (6mA) in C.Â elegans DNA. We further demonstrate that this modification increases trans-generationally in a paradigm of epigenetic inheritance. Importantly, we identify a DNA demethylase, NMAD-1, and a potential DNA methyltransferase, DAMT-1, which regulate 6mA levels and crosstalk between methylations of histone H3K4 andÂ adenines and control the epigenetic inheritance ofÂ phenotypes associated with the loss of the H3K4me2 demethylase spr-5. Together, these data identify a DNA modification in C.Â elegans and raise the exciting possibility that 6mA may be a carrier of heritable epigenetic information in eukaryotes.},
  doi =       {10.1016/j.cell.2015.04.005},
  issn =      {0092-8674},
  owner =     {bram},
  timestamp = {2016-02-29}
}

@Article{Greer2014,
  author    = {Greer, Eric L. and Beese-Sims, Sara E. and Brookes, Emily and Spadafora, Ruggero and Zhu, Yun and Rothbart, Scott B. and Aristiz{\'a}bal-Corrales, David and Chen, Shuzhen and Badeaux, Aimee I. and Jin, Qiuye and Wang, Wei and Strahl, Brian D. and Colai{\'a}covo, Monica P. and Shi, Yang},
  title     = {A histone methylation network regulates transgenerational epigenetic memory in {\emph{\uppercase{C}. elegans}}},
  journal   = {Cell Rep.},
  year      = {2014},
  volume    = {7},
  number    = {1},
  pages     = {113--126},
  month     = apr,
  issn      = {2211-1247},
  abstract  = {Summary How epigenetic information is transmitted from generation to generation remains largely unknown. Deletion of the C. elegans histone H3 lysine 4 dimethyl (H3K4me2) demethylase spr-5 leads to inherited accumulation of the euchromatic H3K4me2 mark and progressive decline in fertility. Here, we identified multiple chromatin-modifying factors, including H3K4me1/me2 and H3K9me3 methyltransferases, an H3K9me3 demethylase, and an H3K9me reader, which either suppress or accelerate the progressive transgenerational phenotypes of spr-5 mutant worms. Our findings uncover a network of chromatin regulators that control the transgenerational flow of epigenetic information and suggest that the balance between euchromatic H3K4 and heterochromatic H3K9 methylation regulates transgenerational effects on fertility.},
  doi       = {10.1016/j.celrep.2014.02.044},
  owner     = {bram},
  timestamp = {2015-08-20},
  url       = {http://dx.doi.org/10.1016/j.celrep.2014.02.044},
}

